{
    "manifest_version": 3,
    "name": "YTcomments",
    "version": "2.0",
    "description": "A simple extension that adds a button to the YouTube search page",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"],
        "run_at": "document_start"
      }
    ]
    
}
  